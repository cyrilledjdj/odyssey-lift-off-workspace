{
  "npmScope": "odyssey-lift-off-workspace",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "api": {
      "tags": []
    },
    "nest-api": {
      "tags": []
    },
    "ng-lift-off": {
      "tags": []
    },
    "ng-lift-off-e2e": {
      "tags": [],
      "implicitDependencies": ["ng-lift-off"]
    },
    "odyssey-lift-off": {
      "tags": []
    },
    "odyssey-lift-off-e2e": {
      "tags": [],
      "implicitDependencies": ["odyssey-lift-off"]
    },
    "utils": {
      "tags": []
    }
  }
}
