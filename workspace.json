{
  "version": 2,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ng-lift-off": {
      "projectType": "application",
      "root": "apps/ng-lift-off",
      "sourceRoot": "apps/ng-lift-off/src",
      "prefix": "ng-lift-off",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ng-lift-off",
            "index": "apps/ng-lift-off/src/index.html",
            "main": "apps/ng-lift-off/src/main.ts",
            "polyfills": "apps/ng-lift-off/src/polyfills.ts",
            "tsConfig": "apps/ng-lift-off/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/ng-lift-off/src/favicon.ico",
              "apps/ng-lift-off/src/manifest.json",
              "apps/ng-lift-off/src/assets"
            ],
            "styles": ["apps/ng-lift-off/src/styles.scss"],
            "scripts": [],
            "aot": true,
            "buildOptimizer": true,
            "sourceMap": true,
            "namedChunks": true,
            "vendorChunk": true,
            "extractCss": true,
            "outputHashing": "all",
            "baseHref": "/",
            "deployUrl": ""
          },
          "configurations": {
            "production": {
              "sourceMap": false,
              "namedChunks": false,
              "baseHref": "/ng",
              "deployUrl": "ng/",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/ng-lift-off/src/environments/environment.ts",
                  "with": "apps/ng-lift-off/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ng-lift-off:build:production",
              "port": 4201
            },
            "development": {
              "browserTarget": "ng-lift-off:build:development",
              "port": 4201
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-lift-off:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ng-lift-off/src/**/*.ts",
              "apps/ng-lift-off/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ng-lift-off"],
          "options": {
            "jestConfig": "apps/ng-lift-off/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ng-lift-off-e2e": {
      "root": "apps/ng-lift-off-e2e",
      "sourceRoot": "apps/ng-lift-off-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ng-lift-off-e2e/cypress.json",
            "devServerTarget": "ng-lift-off:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-lift-off:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/ng-lift-off-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "odyssey-lift-off": {
      "root": "apps/odyssey-lift-off",
      "sourceRoot": "apps/odyssey-lift-off/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/odyssey-lift-off",
            "index": "apps/odyssey-lift-off/src/index.html",
            "main": "apps/odyssey-lift-off/src/main.tsx",
            "polyfills": "apps/odyssey-lift-off/src/polyfills.ts",
            "tsConfig": "apps/odyssey-lift-off/tsconfig.app.json",
            "assets": [
              "apps/odyssey-lift-off/src/robots.txt",
              "apps/odyssey-lift-off/src/manifest.json",
              "apps/odyssey-lift-off/src/favicon.ico",
              "apps/odyssey-lift-off/src/assets"
            ],
            "styles": ["apps/odyssey-lift-off/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/odyssey-lift-off/src/environments/environment.ts",
                  "with": "apps/odyssey-lift-off/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "odyssey-lift-off:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "odyssey-lift-off:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/odyssey-lift-off/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/odyssey-lift-off"],
          "options": {
            "jestConfig": "apps/odyssey-lift-off/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "odyssey-lift-off-e2e": {
      "root": "apps/odyssey-lift-off-e2e",
      "sourceRoot": "apps/odyssey-lift-off-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/odyssey-lift-off-e2e/cypress.json",
            "devServerTarget": "odyssey-lift-off:serve",
            "tsConfig": "apps/odyssey-lift-off-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "odyssey-lift-off:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/odyssey-lift-off-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils"],
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "odyssey-lift-off"
}
